<div class="tab-content">
    <div class="tab-pane" id="sectionOne" role="tabpanel" aria-labelledby="home-tab">

        <div class="card card-one mt-3">
            <div class="card-header">
                <h6 class="card-title center">Forecast - Total Portafolio</h6>
                <nav class="nav nav-icon nav-icon-sm ms-auto">
                    <a href="" class="nav-link"><i class="ri-refresh-line"></i></a>
                    <a href="" class="nav-link"><i class="ri-more-2-fill"></i></a>
                </nav>
            </div><!-- card-header -->
            <div class="card-body p-3">
                <div class="table-responsive">
                    <table class="table table-borderless mb-0 dataTable-loading searchable">
                        <thead>
                            <tr>
                                <th></th>
                                @foreach (var item in Model)
                                {
                                    <th>@item.DescripcionPeriodo</th> <th></th>
                                }
                            </tr>
                        </thead>
                        <tbody>
                            <thead class="border border-2">
                                <tr>
                                    <th>1-Proyección Ventas</th>
                                    @foreach (var item in Model)
                                    {
                                        <th>@item.PrimaModerada.ToString("C", System.Globalization.CultureInfo.CreateSpecificCulture("en-US"))</th> 
                                        <th><i class="ri-arrow-up-circle-line bg-success" data-bs-toggle="tooltip" title="" data-bs-original-title="ri-arrow-up-circle-line" aria-label="ri-arrow-up-circle-line"></i></th>
                                    }
                                </tr>
                                <tr>
                                    <th>2-Proyección Renovaciones</th>
                                    @foreach (var item in Model)
                                    {
                                        <th>@item.RenovacionModerada.ToString("C", System.Globalization.CultureInfo.CreateSpecificCulture("en-US")) </th>
                                        <th></th>
                                    }
                                </tr>
                            </thead>
                            <tr>
                                <th>3-Ventas Real</th>
                                @foreach (var item in Model)
                                {
                                    <th><a href="~/stages/details?CodigoPeriodo=@item.CodigoPeriodo&CodigoTipoVenta=01"> @item.Cobrado.ToString("C", System.Globalization.CultureInfo.CreateSpecificCulture("en-US"))</a></th>
                                    <th></th>
                                }
                            </tr>
                            <tr>
                                <th>4-Renovaciones Real</th>
                                @foreach (var item in Model)
                                {
                                    <th><a href="~/stages/details?CodigoPeriodo=@item.CodigoPeriodo&CodigoTipoVenta=02"> @item.RenovacionCobrado.ToString("C", System.Globalization.CultureInfo.CreateSpecificCulture("en-US"))</a></th>
                                    <th></th>
                                }
                            </tr>
                        </tbody>
                        <tfoot>
                            <tr>
                                <th>5-Total</th>
                                @foreach (var item in Model)
                                {
                                    var Total = (@item.Cobrado + @item.RenovacionCobrado);
                                    <th>
                                        <a href="~/stages/TotalPeriodo?CodigoPeriodo=@item.CodigoPeriodo">
                                        @Total.ToString("C", System.Globalization.CultureInfo.CreateSpecificCulture("en-US"))
                                    </a>
                                </th>
                                <th></th>
                                }
                            </tr>

                        </tfoot>
                    </table>
                </div>
            </div><!-- card-body -->
        </div>
