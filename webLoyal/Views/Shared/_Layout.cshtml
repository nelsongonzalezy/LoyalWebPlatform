<script>
    $(document).ready(function () {
        var activeLinkId = sessionStorage.getItem('activeLinkId');
        if (activeLinkId) {
            $('#' + activeLinkId).addClass('active');
            $('#' + activeLinkId).closest('.nav-group').addClass('show');
        }

        $('.nav-link').on('click', function (e) {
            e.preventDefault();
            $('.nav-link').removeClass('active');
            $('.nav-group').removeClass('show');

            $(this).addClass('active');
            $(this).closest('.nav-group').addClass('show');

            sessionStorage.setItem('activeLinkId', $(this).attr('id'));

            var href = $(this).attr('href');
            if (href && href !== '#') {
                window.location.href = href;
            }
        });
    });

</script>




